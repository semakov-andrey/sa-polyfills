{
  "name": "sa-polyfills",
  "version": "1.2.1",
  "description": "Front-End polyfills",
  "keywords": [
    "js",
    "es6",
    "polyfill",
    "umd",
    "library"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:semakov-andrey/sa-polyfills.git"
  },
  "author": "Semakov Andrey <semakov.andrey.web@yandex.ru>",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "del": "^3.0.0",
    "eslint": "^5.12.0",
    "extract-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "image-webpack-loader": "^4.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-assets-webpack-plugin": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.3",
    "pug-html-loader": "^1.1.5",
    "style-loader": "^0.23.1",
    "stylelint": "^9.9.0",
    "svg-fill-loader": "0.0.8",
    "svg-sprite-loader": "^4.1.3",
    "svgo-loader": "^2.2.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.28.4",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "scripts": {
    "start": "node start.js",
    "build": "node build.js",
    "module": "node node_modules/sa-template-2/module.js",
    "lint-js": "eslint src/**/*.js",
    "lint-js-fix": "eslint src/**/*.js --fix",
    "lint-css": "stylelint src/**/*.css -f verbose",
    "lint-css-fix": "stylelint src/**/*.css -f verbose --fix"
  },
  "config": {
    "devServer": {
      "secure": false,
      "host": "0.0.0.0",
      "port": "3000"
    },
    "entries": {
      "test": "test",
      "sa-closest": "sa-closest",
      "sa-detection": "sa-detection",
      "sa-grid-auto-placement": "sa-grid-auto-placement"
    },
    "directories": {
      "source": "src",
      "development": "tmp",
      "production": "build",
      "files": {
        "html": "",
        "css": "styles",
        "js": "scripts",
        "fonts": "fonts",
        "images": "images",
        "sprite": "images",
        "other": ""
      }
    }
  },
  "browserslist": [
    "> 0.5%"
  ]
}
